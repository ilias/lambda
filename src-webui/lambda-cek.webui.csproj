    </Content>
    <Content Include="..\tests.lambda">
      <Link>tests.lambda</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="..\submodA.lambda">
      <Link>submodA.lambda</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="..\submodB.lambda">
      <Link>submodB.lambda</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="..\testmod.lambda">
      <Link>testmod.lambda</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
</Project>
